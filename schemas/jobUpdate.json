{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/jobUpdate.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "companyHandle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "j1"
            ]
        },
        "salary": {
            "type": "number",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                10
            ]
        },
        "equity": {
            "type": "string",
            "default": "0.0",
            "title": "The equity Schema",
            "examples": [
                "0.5"
            ]
        }
    },
    "examples": [{
        "title": "j1",
        "salary": 10,
        "equity": 0.5
    }]
}